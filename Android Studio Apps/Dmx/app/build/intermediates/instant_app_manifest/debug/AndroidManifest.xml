<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="deliled.applications.android.dmx"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="deliled.applications.android.dmx.Welcome" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="deliled.applications.android.dmx.Scan"
            android:screenOrientation="portrait" />
        <activity
            android:name="deliled.applications.android.dmx.Qrcode"
            android:screenOrientation="portrait" />
        <activity
            android:name="deliled.applications.android.dmx.SettingsDmx"
            android:screenOrientation="portrait" />
        <activity
            android:name="deliled.applications.android.dmx.Warningone"
            android:screenOrientation="portrait" />
        <activity
            android:name="deliled.applications.android.dmx.Warningtwo"
            android:screenOrientation="portrait" />
        <activity
            android:name="deliled.applications.android.dmx.Countdownuvc"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>